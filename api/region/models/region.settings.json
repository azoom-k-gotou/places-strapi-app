{
  "kind": "collectionType",
  "collectionName": "region",
  "info": {
    "name": "Region"
  },
  "options": {
    "increments": true,
    "timestamps": ["created_datetime", "updated_datetime"]
  },
  "attributes": {
    "city_id": {
      "model": "city",
      "via": "regions"
    },
    "lat": {
      "type": "decimal",
      "required": true
    },
    "lng": {
      "type": "biginteger",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "name_furigana": {
      "type": "string",
      "required": true
    }
  }
}
